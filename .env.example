# Example .env file for Gemini MCP Server

# Required: Your Google Gemini API key
# Get it from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your-api-key-here

# Optional: Gemini model to use (default: gemini-1.5-flash)
# Options: gemini-1.5-flash, gemini-1.5-pro, gemini-1.0-pro
GEMINI_MODEL=gemini-1.5-flash

# Optional: Output directory for generated files (default: /tmp/gemini_mcp)
GEMINI_OUTPUT_DIR=/tmp/gemini_mcp
