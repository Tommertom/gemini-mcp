{
  "name": "gemini-image-mcp-server",
  "version": "0.3.2",
  "description": "MCP server for Google Gemini media operations",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "gemini-mcp-server": "./dist/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && chmod 755 dist/index.js",
    "doPublish": "npm run build && npm version patch &&npm publish --access public",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "tsx scripts/test-mcp-server.ts",
    "test:verbose": "DEBUG=1 tsx scripts/test-mcp-server.ts",
    "test:keep-files": "tsx scripts/test-mcp-server.ts --preserve-files"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "gemini",
    "google-ai",
    "media",
    "image-generation",
    "video-analysis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@modelcontextprotocol/sdk": "^1.20.0",
    "dotenv": "^17.2.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
